FRAMEWORK_SEARCH_PATHS = $(PODS_ROOT)/google-cast-sdk/GoogleCastFramework-2.3.0-Release
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/google-cast-sdk"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/google-cast-sdk"
OTHER_LDFLAGS = -ObjC -l"Pods-google-cast-sdk" -framework "GoogleCast" -framework "MediaAccessibility"
PODS_ROOT = ${SRCROOT}/Pods